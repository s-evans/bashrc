[user]
    name = 
    email = 
[alias]
    st = status
    ci = commit
    cm = commit -m
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    lol = log --graph --decorate --pretty-oneline --abbrev-commit
    lola = log --graph --decorate --pretty-oneline --abbrev-commit --all
    ls = ls-files
    ign = status --ignored
[core]
    excludesFile = ~/.gitignore_global
[merge]
    tool = vimdiff
[diff]
    tool = vimdiff
    renames = copies
    noprefix = true
[push]
    default = simple
[rerere]
    autoUpdate = true
    enabled = true
[branch]
    autoSetupMerge = true
